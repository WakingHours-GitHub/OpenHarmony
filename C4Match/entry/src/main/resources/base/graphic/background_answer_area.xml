<?xml version="1.0" encoding="utf-8"?>

<!--设置形状为rectangle, 即矩形-->
<shape
    xmlns:ohos="http://schemas.huawei.com/res/ohos"

    ohos:shape="rectangle">

    <corners ohos:radius="10vp" />

<!--    背景颜色-->
    <solid ohos:color="#FFA7A7A7"/>


<!--    设置边框的宽度, 和颜色-->
    <stroke
        ohos:width="2vp"
        ohos:color="#FF6D6D6D"
        />

</shape>